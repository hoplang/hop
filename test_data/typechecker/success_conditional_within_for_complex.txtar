-- main.hop --
<component name="main" params-as="params">
	<for each="params.sections" as="section">
		<cond if="section.header.visible">
			<for each="section.items" as="item">
				<cond if="item.data.valid">
				</cond>
			</for>
		</cond>
	</for>
</component>
-- out --
main::main : {sections: {header: {visible: boolean}, items: {data: {valid: boolean}}[]}[]}
